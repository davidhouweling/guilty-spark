---
import type { ImageMetadata } from "astro";
import StatsCommandGif from "../../assets/recording-20250604-stats-command.gif";

const image = StatsCommandGif as ImageMetadata;
---

<img
  src={image.src}
  width={image.width}
  height={image.height}
  alt="Stats command demo showing match statistics retrieval"
  loading="lazy"
  class="demo-image"
  decoding="async"
  fetchpriority="low"
/>
