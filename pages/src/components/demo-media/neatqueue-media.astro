---
import type { ImageMetadata } from "astro";
import NeatQueueIntegrationGif from "../../assets/recording-20250604-neatqueue-integration.gif";

const image = NeatQueueIntegrationGif as ImageMetadata;
---

<img
  src={image.src}
  width={image.width}
  height={image.height}
  alt="NeatQueue integration showing automated stats posting"
  loading="lazy"
  class="demo-image"
  decoding="async"
  fetchpriority="low"
/>
